<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="WhitespaceAround" files="[\\/]api[\\/]"/>
    <suppress checks="WhitespaceAfter" files="[\\/]api[\\/]"/>
    <suppress checks="SingleSpaceSeparator" files="[\\/]api[\\/]"/>
    <suppress checks="NoWhitespaceBefore" files="[\\/]api[\\/]"/>
    <suppress checks="NoWhitespaceAfter" files="[\\/]api[\\/]"/>
    <suppress checks="GenericWhitespace" files="[\\/]api[\\/]"/>
    <!--  Let javax.* stand before java.* - that's default Idea settings  -->
    <suppress checks="CustomImportOrder"
              message="Wrong lexicographical order for 'java\.[^']+' import\. Should be before 'javax\.[^']+'\."/>
</suppressions>
